{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "session": {
      "type": "string"
    },
    "events": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "type": {
              "type": "string"
            },
            "payload": {
              "type": "object",
              "properties": {
                "timestamp": {
                  "type": "integer"
                },
                "currentTime": {
                  "type": "integer"
                },
                "duration": {
                  "type": "integer"
                }
              },
              "required": [
                "timestamp",
                "currentTime",
                "duration"
              ]
            }
          },
          "required": [
            "type",
            "payload"
          ]
        }
      ]
    }
  },
  "required": [
    "session",
    "events"
  ]
}